class Node{
    constructor(val){
        this.val = val;
        this.next = null;
        this.prev = null;
    }
}

class DoublyLinkedList{
  constructor(){
      this.head = null;
      this.tail = null;
      this.length = 0;
  }
  
  push(val){
    var newNode = new Node(val);
    
    if(this.length === 0){
      this.head = newNode;
      this.tail = newNode;
    }else{
      this.tail.next = newNode;
      newNode.prev = this.tail;
      this.tail = newNode;
    }
    this.length++
  }
  
  pop(){
    if(this.length === 0)return false;
    if(this.length === 1 ) return this.head;
    
    var originalTail = this.tail;
    originalTail.prev = null;
    this.length--;
    return originalTail;
  }
  
  
}

const DLL = new DoublyLinkedList();
DLL.push("A");
DLL.push("B");
DLL.push("C");
DLL.pop();
DLL

var arr = [1,4,5,6];


